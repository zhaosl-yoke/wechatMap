@import "../../libs/common.wxss";
@import '../../libs/icon.wxss';
page{
    height: 100%;
	width: 100%;
	font-size: 28rpx;
	font-family:"苹方-简","华文细黑","微软雅黑", "Microsoft YaHei","sans-serif";
	background: #fff;	
	color: #333;
}
.container{
    padding:0;
    justify-content: flex-start;
}
ul,li,ol{
	list-style: none;
}
.loading{
	position: absolute;
	width: 80rpx;
	top: 50%;
	left: 50%;
	margin-left: -40rpx;
	margin-top: -40rpx;
	z-index: 999;
}
 radio{
	/* visibility: hidden; */
} 
.mui-ellipsis-1{
	display: -webkit-box;
    overflow: hidden;
    white-space: normal!important;
    text-overflow: ellipsis;
    word-wrap: break-word;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    line-height: 40rpx;
}
.shadow{
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	background: rgba(0,0,0,0.5);
	z-index: 99;
}
.shadow_con {
	position: absolute;
	top: 400rpx;
	left: 50%;
	margin-left: -355rpx;
	z-index: 999;
	width: 710rpx;
	padding: 80rpx 15rpx 20rpx 15rpx;
	border-radius: 5rpx;
	background: #fff;
	box-sizing: border-box;
	height: 500rpx;
	overflow: auto;
}
.shadow_con image{
	position: absolute;
	width: 60rpx;
	left: 20rpx;
	top: 25rpx;
	padding: 10rpx;
    box-sizing: border-box;
}
.shadow_con ul{
	display: flex;
	flex-wrap: wrap;
}
.shadow_con ul li{
	list-style: none;
	display: inline-block;
	padding: 0 20rpx;
	height: 60rpx;
	border: solid 1px #ccc;
	text-align: center;
	line-height: 60rpx;
	border-radius: 5rpx;
	margin: 20rpx 5rpx;
	font-size: 24rpx;
	box-sizing: border-box;
}
.shadow_con ul .active{
	color: #f44a33;
	border: solid 1px #f44a33;
}
#container{
	display: flex;
	flex-direction: column;
	height: 100%;
	width: 100%;
	padding-top: 10rpx;
	background: #f8f8fa;
	overflow: hidden;
}
#map{
	flex: 1;
	border-top: solid 1px #ccc;
}
.box{
   	 width: 100%;
	 height: 80rpx;
	 flex: 0 0 80rpx;
	 padding: 0 20rpx 0 20rpx;
     box-sizing: border-box;
	 display: flex;
	 align-items:center;
	 background: #f8f8fa;
}
.box .searchBox{
	 border-radius: 5rpx;
	 height: 62rpx;
	 width: 510rpx;
	 display: flex;
	 align-items:center;
	 background: #ececec;
	 margin-right: 40rpx;
}
.searchBox #search{
   	width: 25rpx;
 	margin-right: 20rpx;
 	margin-left: 20rpx;
}
.searchBox #autotext{
	 border: none;
	 outline: none;
	 margin-left: 25rpx;
	 flex: 1;
	 background: #ececec;
}
.box .add{
	width: 160rpx;
	height: 62rpx;
	text-align: center;
	line-height: 62rpx;
	background: #f44a33;
	color: #fff;
	border-radius: 5rpx;
}
.tab_list{
    width: 100%;
	flex: 0 0 80rpx;
	display: flex;
    padding: 0 20rpx;
    box-sizing: border-box;
	background: #f8f8fa;
	height: 80rpx;
	align-items: center;
	justify-content: space-between;
}
.tab_list .all{
	letter-spacing: 1rpx;
	border: solid 1px #ccc;
	height: 60rpx;
	padding: 0 20rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	border-radius: 50rpx;
}
.tab_list .all image{
	width: 16rpx;
}
.tab_list view image{
	width: 40rpx;
    vertical-align: middle;
    margin-left: 10rpx;
}
.tab_list .active{
	color: #f44a33;
}
.sort_type {
	flex: 0 0 100rpx;
	height: 100rpx;
	background: #fff;
	display: flex;
	align-items: center;
	padding: 0 60rpx;
}
.sort_type view{
	width: 176rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	border: solid 1px #cccccc;
	font-size: 22rpx;
	border-radius: 50rpx;
}
.sort_type .distance{
	margin-right: 108rpx;
}
.sort_type .amount{
	margin-right: 118rpx;
}
.sort_type image{
	width: 45rpx;
}
.sort_type .active{
	border: solid 1px #f44a33;
	color: #f44a33;
}
#list{
	flex: 1;
    width: 100%;
	border-top: solid 1px #ccc;
	padding-left: 20rpx;
	background: #fff;
	overflow: auto;
    padding-right: 10rpx;
    box-sizing: border-box;
}
#list .list_con li{
	display: flex;
	margin-top: 30rpx;
}
#list .none{
    display: block;
    text-align: center;
    line-height: 100rpx;
}
.list_con li .poster{
	width: 150rpx;
	height: 200rpx;
	border-radius: 5rpx;
	border: 1px dashed #989898;
    background: #eee;
	margin-right: 30rpx;
	flex: 0 0 150rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
}
.list_con li .poster text{
	font-size: 24rpx;
	margin-top: 10rpx;
    color: #999;
}
.list_con li .poster .add_pic{
	width: 50rpx;
}
.list_con li .poster .pic{
	width: 150rpx;
}
.detail .inst_name{
	margin: 15rpx 0 15rpx 0;
	padding: 5rpx 0;
}
.detail .inst_name .flag{
	border: solid 1px #f44a33;
	text-align: center;
	line-height: 24rpx;
	border-radius: 5rpx;
	color: #f44a33;
	font-size: 22rpx;
	padding: 2rpx 6rpx;
	margin-right: 15rpx;
}
.detail .address{
	margin-bottom: 30rpx;
	color: #666;
}
.detail .data{
	display: flex;
	align-items: center;
}
.detail .data .span{
	display: flex;
	align-items: center;
}
.detail .data image{
	width: 30rpx;
	margin-right: 10rpx;
}
.data span{
	color: #989898;
}
.backTop{
   	display: none;
   	position: fixed;
   	top: 90%;
   	right: 20px;
}
.listCon{
   	position: relative;
}
.list{
   	text-align:center;
   	width:100%;
   	font-size:28rpx;
   	height: 86rpx;
   	line-height: 80rpx; 
}


.animate{
  width: 100%;
  z-index: 2;
  height: 84rpx;
  position: absolute;
  right: -640rpx;
  top: 40rpx;
  background: rgba(116,163,237,0.7);
  border-radius: 40rpx 0 0 40rpx;
  display: flex;
  align-items: center;
  font-size: 28rpx;
}
.less{ 
  width: 48rpx;
  height: 48rpx;
}
.less1{
  margin-left: 20rpx;
}
.less2{
  margin-left: -20rpx;
}
.eidt{
  align-self: center;
  color: #fff;
  margin-left: 20rpx;
}
.close_icon:after{
  font-family:"iconfont" !important;
  font-size: 40rpx;
  font-style:normal;
  content: '\e600';
  margin-left: 40rpx;
  color: #fff;
  font-weight: normal;
}
.login2{
  display: inline;
  font-size: 30rpx;
}

.info {
    padding: 0.2rpx;
    min-width: 200px;
}
div.info-top {
     position: relative;
     background: none repeat scroll 0 0 #F9F9F9;
     border-bottom: 1px solid #CCC;
     border-radius: 5px 5px 0 0;
     font-size: 0.24rpx;
}
div.info-top div {
    display: inline-block;
    color: #333333;
    font-size: 14px;
    font-weight: bold;
    line-height: 31px;
    padding: 0 10px;
}
div.info-top image {
    position: absolute;
    top: 10px;
    right: 10px;
    transition-duration: 0.25s;
}
div.info-top image:hover {
    box-shadow: 0px 0px 5px #000;
}
div.info-middle {
    font-size: 12px;
    line-height: 4px;
    padding: 20rpx;
    padding-top: 25rpx;
    box-sizing: border-box;
}
.content_height{
	line-height: 40rpx;
}
div.info-bottom {
    height: 0px;
    width: 100%;
    clear: both;
    text-align: center;
}
div.info-bottom image {
    position: relative;
    z-index: 104;
}
.school{
	font-size: 30rpx;
}
.right span{
	color: #666;
}
.right .major{
	height: 34rpx;
	margin-right: 10rpx;
	padding-right: 10rpx;
	border-right: solid 1px #ededed;
}
.majorName{
	margin-right: 10rpx;
	padding-right: 10rpx;
	border-right: solid 1px #ededed;
}
.right .last{
	border: none;
	margin-right: 20rpx;
}

.info-middle image {
      float: left;
      margin-right: 6px;
}
.closeMsg{
	position: absolute;
    right: 40rpx;
    top: 35rpx;
}


